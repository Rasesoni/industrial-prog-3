#include <iostream>
#include <fstream>
#include <vector>
#include <algorithm>
using namespace std;

int main() {
    ifstream in("input.txt");
    ofstream out("output.txt");
    vector < int > number;
    int x;
    while (in >> x) {
        number.push_back(x);
    }
    sort(number.begin(), number.end()); // функция
    int median = 0;
    int n = number.size();
    if (n % 2 == 0) {
        median = number[n / 2];
    }
    else {
        median = number[(n-1)/2];
    }
    out << median;
    return 0;
    in.close();
    out.close();
}
