
#include <iostream>
using namespace std;
int generatebin(int n, string stroka=""){
    if (n==0){
        cout << stroka << endl;
        return 0;
    }
    generatebin(n-1, stroka + '0');
    generatebin(n-1, stroka + '1');
    return 0;
}
int main()
{
    int n = 0;
    cin >> n;
    cout << generatebin(n);
    
    return 0;
}
